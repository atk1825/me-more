<%= render "shared/second_header"%>
<div class="main">
  <div class="devise_main">

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :メールアドレス, class:"f_label" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"f_box" %>
  </div>

  <div class="field">
    <%= f.label :パスワード, class:"f_label" %><br />
    <%= f.password_field :password, autocomplete: "current-password", class:"f_box"%>
  </div>

  <div class="actions">
    <%= f.submit "ログイン", class:"f_label" %>
  </div>
<% end %>
<%= render "devise/shared/links" %>
</div>
